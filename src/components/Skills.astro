---
import { developSkills } from "../services/info.json";
import H2 from "../components/headers/H2.astro";
---

<section id="skills" class="align-center py-8">
  <H2>My Skills</H2>
  <div class="flex flex-row flex-wrap justify-center py-6 gap-4">
    {
      developSkills.map((skill) => (
        <img
          class="object-scale-down h-24 w-24 md:h-28 md:w-28"
          loading="lazy"
          src={skill.src}
          alt={skill.alt}
        />
      ))
    }
  </div>
</section>
